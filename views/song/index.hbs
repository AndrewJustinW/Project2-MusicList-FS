<main>


<h1>List Of Songs</h1>

{{#each songs}}

<div>
    <ul>
        <li>
        {{SongName}}{{user._id}}
        <a href="/user/{{userId}}/artist/{{artistId}}/song/{{_id}}">View</a>

        <a href="/user/{{userId}}/artist/{{artistId}}/song/{{_id}}/edit"> Edit</a> 
        <form action="/user/{{userId}}/artist/{{artistId}}/song/{{_id}}?_method=DELETE" method="POST"> <input type="submit" value="Delete"> </form><br>
            
        </li>
    </ul>
</div>
{{/each}}
<a href="/user/{{user._id}}/artist/{{artist._id}}/song/new">Add Song</a>
</main>